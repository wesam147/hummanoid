<?xml version='1.0'?>
<robot name="myrobot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find hummanoid)/urdf/my_robot.gazebo" />

    <link name='chassis'>
        <pose>0 0 0.1 0 0 0</pose>
        <inertial>
            <mass value="10.0" />
            <origin xyz="0.0 0 0.1" rpy=" 0 0 0" />
            <inertia
                ixx="0.1" ixy="0" ixz="0"
                iyy="0.1" iyz="0"
                izz="0.1"
            />
        </inertial>
        <collision name='collision'>
            <geometry>
                <box size=".984 .65 .095" />
            </geometry>
        </collision>
        <visual name='chassis_visual'>
            <origin xyz="0 0 0" rpy=" 0 0 0" />
            <geometry>
                <box size=".984 .65 .095" />
            </geometry>
        </visual>
    </link>
    <link name='lidar_bed'>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.1 0.1 0.1" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.1 0.1 0.1" />
            </geometry>
            <material>
                
            </material>
        </visual>
    </link>
    <joint name='lidar_bed_to_chassis' type='fixed'>
        <origin xyz="1.4 1.0 1.05" rpy="0 0 0" />
        <child link="lidar_bed" />
        <parent link="chassis" />
        <axis xyz="0 0 0" rpy="0 0 0" />
    </joint>
    <link name='caster_right'>
        <collision>
            <origin xyz="0 0 0" rpy=" 0 0 0" />
            <geometry>
                <sphere radius="0.07" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0" rpy=" 0 0 0" />
            <geometry>
                <sphere radius="0.07" />
            </geometry>
        </visual>
    </link>
    <joint name="caster_right_to_chassis" type="fixed">
        <origin xyz="0.367135 0.2 -0.05" rpy="0 0 0" />
        <child link="caster_right" />
        <parent link="chassis" />
        <axis xyz="0 1 0" rpy="0 0 0" />
        <joint_properties damping="0.0" friction="0.0" />
    </joint>
    <link name='caster_left'>
        <collision>
            <origin xyz="0 0 0" rpy=" 0 0 0" />
            <geometry>
                <sphere radius="0.07" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0" rpy=" 0 0 0" />
            <geometry>
                <sphere radius="0.07" />
            </geometry>
        </visual>
    </link>
    <joint name='caster_leftt_to_chassis' type='fixed'>
        <origin xyz="0.367135 -0.2 -0.05" rpy="0 0 0" />
        <child link="caster_left" />
        <parent link="chassis" />
        <axis xyz="0 1 0" rpy="0 0 0" />
        <joint_properties damping="0.0" friction="0.0" />
    </joint>
    <link name="left_wheel">
        <collision name="collision">
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
            <geometry>
                <cylinder radius="0.125" length="0.065" />
            </geometry>
        </collision>
        <visual name="left_wheel_visual">
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
            <geometry>
                <cylinder radius="0.125" length="0.065" />
            </geometry>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
            <mass value="1" />
            <inertia
                ixx=".1" ixy="0.0" ixz="0.0"
                iyy=".1" iyz="0.0"
                izz=".1" />
        </inertial>
    </link>
    <link name="right_wheel">
        <collision name="collision">
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
            <geometry>
                <cylinder radius="0.125" length="0.065" />
            </geometry>
        </collision>
        <visual name="right_wheel_visual">
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
            <geometry>
                <cylinder radius="0.125" length="0.065" />
            </geometry>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
            <mass value="1" />
            <inertia
                ixx=".1" ixy="0.0" ixz="0.0"
                iyy=".1" iyz="0.0"
                izz=".1" />
        </inertial>
    </link>
    <joint type="continuous" name="left_wheel_hinge">
        <origin xyz="-0.315 -0.355 0.0" rpy="0 0 0" />
        <child link="left_wheel" />
        <parent link="chassis" />
        <axis xyz="0 1 0" rpy="0 0 0" />
        <joint_properties damping="0.0" friction="0.0" />
    </joint>
    <joint type="continuous" name="right_wheel_hinge">
        <origin xyz="-0.315 0.355 0.0" rpy="0 0 0" />
        <child link="right_wheel" />
        <parent link="chassis" />
        <axis xyz="0 1 0" rpy="0 0 0" />
        <joint_properties damping="0.0" friction="0.0" />
    </joint>
    <link name="kinect_link">
        <visual>
            <geometry>
                <box size="0.064 0.121 0.0381" />
            </geometry>
        </visual>
        <collision>
            <geometry>
                <box size="0.064 0.121 0.0381" />
            </geometry>
            <material name="Black" />
        </collision>
        <inertial>
            <mass value="0.0001" />
            <origin xyz="0 0 0" />
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
                iyy="0.0001" iyz="0.0"
                izz="0.0001" />
        </inertial>
    </link>
    <joint name="kinect_depth_joint" type="fixed">
        <origin xyz="0 0.028 0" rpy="0 0 0" />
        <parent link="kinect_link" />
        <child link="kinect_depth_frame" />
    </joint>
    <link name="kinect_depth_frame">
        <inertial>
            <mass value="0.0001" />
            <origin xyz="0 0 0" />
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
                iyy="0.0001" iyz="0.0"
                izz="0.0001" />
        </inertial>
    </link>
    <joint name="depth_optical_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="kinect_depth_frame" />
        <child link="kinect_depth_optical_frame" />
    </joint>
    <link name="kinect_depth_optical_frame">
        <inertial>
            <mass value="0.0001" />
            <origin xyz="0 0 0" />
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
                iyy="0.0001" iyz="0.0"
                izz="0.0001" />
        </inertial>
    </link>
    <joint name="base_link_to_kinect_link" type="fixed">
        <parent link="chassis" />
        <child link="kinect_link" />
        <origin rpy="0 0 0" xyz="0.5 0 0.05155" /><!--0.4-->
    </joint>
    <link name="leg1">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.125" length="0.6" />
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.125" length="0.6" />
            </geometry>
        </collision>
        <inertial>
            <mass value="5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>
    </link>
    <link name="leg2">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.125" length="0.6" />
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.125" length="0.6" />
            </geometry>
            <color rgba="0 0 1 1" />
        </collision>
        <inertial>
            <mass value="5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>
    </link>
    <joint name="base_link_leg1" type="fixed">
        <origin xyz="0.09375 0.105 0.35" rpy="0 0 0" />
        <parent link="chassis" />
        <child link="leg1" />
        <limit effort="30" velocity="1.0" lower="-1.57" upper="1.57" />
        <axis xyz="0 0 1" />
    </joint>
    <joint name="base_link_leg2" type="fixed">
        <origin xyz="0.09375 -0.105 0.35" rpy="0 0 0" />
        <parent link="chassis" />
        <child link="leg2" />
        <limit effort="30" velocity="1.0" lower="-1.57" upper="1.57" />
        <axis xyz="0 0 1" />
    </joint>

    <!-- Hokuyo Laser -->
    <link name="hokuyo_link">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.05" length="0.04" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.05" length="0.04" />
            </geometry>
        </visual>
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
        </inertial>
    </link>
    <joint name="hokuyo_joint" type="fixed">
        <axis xyz="0 1 0" />
        <origin xyz="0.4 0.0 0.114" rpy="0 0 0" />
        <parent link="chassis" />
        <child link="hokuyo_link" />
    </joint>

</robot>
